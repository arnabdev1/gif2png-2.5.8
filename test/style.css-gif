// Peter Gale's stylesheet example, with gtcd.gif substituted
H1, H2, H3, H4, H5, H6, .keys, .huge, .due, .links, .link1, .footer, .linkc {
	font:medium Tahoma, Verdana, Helvetica, Arial, sans-serif, "MS Sans Serif";
	color:black;
}

BODY, .block, .t1, .t2, .t3, .t4, caption, .heading, .aside, TD, TABLE, TR, BLOCKQUOTE, UL, OL, LI {
	font:medium "Times New Roman", Times, "Bookman Old Style", Bookman, "Book Antiqua", Serif, "MS Serif";
	color:black;
	line-height:1.0;
	font-size:12pt;
}

BODY {	
	background:white url(gtcd.gif) repeat fixed;
	margin-top:5px;
	margin-left:5px;
	margin-right:10px;
	position:absolute;
	left:0;
	top:0;
}

BLOCKQUOTE {
	clear:both;
}

/*
INPUT {
	background-color:#ffcc99;
	font:medium "Courier New", Monaco, Clean, Courier, monospace;
	font-size:9pt;
}
*/

CODE {
	clear:both;
	color:black;
	background-color:#ffcc99;
	margin:10px 0px 10px 0px;
	font:medium "Courier New", Monaco, Clean, Courier, monospace;
	font-size:9pt;
	text-align:left;
}

PRE	{
	clear:both;
	color:black;
	font:medium "Courier New", Monaco, Clean, Courier, monospace;
	font-size:9pt;
	margin:0px;
	text-align:left;
}

IMG, A IMG	{
	clear:both;
	padding:1px;

}

.block {
	text-align:left;
	margin-top:0px;
	margin-bottom:10px;
	margin-left:25px;
	margin-right:25px;

}

.links {
	font-size:10pt;
	color:black;
}

.link1 {
	font-size:12pt;
	font-weight:bold;
	color:black;
}

.linkc {
	font-size:10pt;
	color:black;
	font-weight:bold;
}

.keys {
	color:#000000;
	background-color:#ffcc99;
	font-weight:normal;
	font-size:10pt;
	font-style:italic;
}

.t1 {
	line-height:normal;
}	

.t2 {
	background-color:#ffcc99;
	color:black;
	line-height:normal;
}	

.t3 {
	background-color:#cc9966;
	color:#ffcc99;
	text-align:center;
	line-height:normal;
}	

.t4 {
	background-color:#ffcc99;
	color:black;
	text-align:center;
	line-height:normal;
}	

.aside {
	color:#cc9966;
	font-style:italic;
}

.footer {
	color:#cc9966;
	text-align:right;
	font-size:x-small;
}

.date {
	text-align:center;
	font-size:x-large;
	line-height:normal;
	color:#ffcc99;
/*	font-family:gothic, fantasy, sans-serif;
	font-family:gothic, fantasy, sans-serif;   */
	background-color:#cc9966;
	border:thin groove #cc9966;
}

.huge {
	text-align:center;
	font-size:72pt;
	line-height: normal;
	color:#ffcc99;
/*	font-family:gothic, fantasy, sans-serif;  */
	background-color:#cc9966;
	border:thin double #cc9966;
}

H1 {
	text-align:center;
	font-size:18pt;
	line-height:normal;
	color:#663300;
/*	font-family:gothic, fantasy, sans-serif;
	font-family:"Franklin Gothic ITC Demi",gothic, fantasy; */
	margin-bottom:0px;
	position:relative;
	top:-50px;
}

H2 {
	text-align:center;
	font-size:14pt;
	color:#000000;
	margin-top:-50px;
/*	font-family:script,gothic, fantasy;*/
}

H3 {
	color:#cc9966;
	font-weight:bold;
	font-size:12pt;
}

H4 {
	color:#663300;
	font-size:11pt;
}

H5 {
	color:#996633;
	font-size:10pt;
}

H6 {
	font-size:9pt;
}

.due {
	text-align:center;
	font-size:12pt;
	line-height:0.5;
	background-color:#ffcc99;
	margin-top:0px;
	margin-bottom:0px;
}


UL {
	list-style-position:outside;
	text-align:left;
	margin:0px 40px 10px 40px;
}
/*
UL LI {
	list-style-image:url(ducks.gif);
}
*/

A:link, A:visited, A:active, A:hover { text-decoration:none }

A:link {
	color:#006633;
	font:10pt Tahoma, Verdana, Helvetica, Arial, sans-serif;
}

A:active {
	color:#ffcc99;
	font:10pt Tahoma, Verdana, Helvetica, Arial, sans-serif;
}

A:visited {
	color:#cc9966;
	font:10pt Tahoma, Verdana, Helvetica, Arial, sans-serif;
}

A:hover {
	color:#ffcc99;
	background-color:#cc9966;
	font:10pt Tahoma, Verdana, Helvetica, Arial, sans-serif;
}
